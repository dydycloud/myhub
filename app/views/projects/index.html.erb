<div class="row">
	<div class="col-xs-4 col-sm-3">
		<% @projects.each do |project| %>
			<p>
				<%= link_to project.title, project %>
				<%= link_to '', project_path(project), method: :delete, data: { confirm: 'Are you sure?' },  :class => "glyphicon glyphicon-trash pull-right", :id => "tools"  %>
				<%= link_to '', project,  :class => "glyphicon glyphicon-ok pull-right", :id => "tools"  %>
				<%= link_to '', edit_project_path(project), :class => "glyphicon glyphicon-pencil pull-right", :id => "tools" %>
				
			</p>
		<% end %>
	</div>
	<div class="col-xs-10 col-sm-4 col-md-6">
	
		
	</div>

</div>
